<nav id="nav" class="special" >
            <ul class="menu links" >
			<!-- Homepage  主页  --> 
			<li >
	            <a href="<%= config.root %>" rel="nofollow"><%- theme.Nav.Home_name %></a>
	        </li>
			<!-- categories_name  分类   --> 
	        <% if(theme.Nav.is_use_categories === true) { %>
	        <li class="active">
	            <a href="#s1"><%- theme.Nav.categories_name %></a>
	                    <ul class="submenu">
	                        <li>
	                        <%- list_categories({
	                            show_count: false,
	                            amount: 5,
	                            style: 'none',
	                            separator: '</li><li>'
	                        }) %>
	                    </ul>
	        </li>
	        <% } %>
	        <!-- archives  归档   --> 
	        <% if(theme.Nav.is_use_archives === true) { %>
	        <li class="active">
	            <a href="#s1"><%- theme.Nav.archives_name %></a>
	                    <ul class="submenu">
	                        <li>
	                        <%- list_archives({
	                            show_count: false,
	                            amount: 5,
	                            style: 'none',
	                            separator: '</li><li>'
	                        }) %>
	                    </ul>
	        </li>
	        <% } %>
	        
		        <!-- Pages 自定义   -->
		        <% for (var i in theme.Nav.pages){ %>
		        <li>
		            <a href="<%= theme.Nav.pages[i].link %>" title="<%= i %>">
		                <%= i %>
		            </a>
		        </li>
		        <% } %>


            </ul>
            <!-- icons 图标   -->
			<ul class="icons">
                    <% for (var i in theme.Nav.icon) { %>
                    <li>
                        <a title="<%= i %>" href="<%- url_for(theme.Nav.icon[i]) %>" target="_blank" rel="noopener">
                            <i class="icon fa fa-<%= i %>"></i>
                        </a>
                    </li>
                    <% } %>
			</ul>
</nav>